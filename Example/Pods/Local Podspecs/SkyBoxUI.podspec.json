{
  "name": "SkyBoxUI",
  "version": "0.3.43",
  "summary": "SkyBoxUI UI 插件",
  "description": "SkyBoxUI 使用cocoapods 管理的 UI插件",
  "homepage": "https://github.com/appleMini/SkyBoxUI",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "appleMini": "s1298web@163.com"
  },
  "source": {
    "git": "https://github.com/appleMini/SkyBoxUI.git",
    "tag": "0.3.43"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "source_files": "SkyBoxUI/Classes/**/**/*.{h,m,mm}",
  "resource_bundles": {
    "SkyBoxUI": [
      "SkyBoxUI/Assets/*.{png,pdf,mp4}",
      "SkyBoxUI/Assets/**/*.{png,pdf}",
      "SkyBoxUI/Assets/**/*.ttf",
      "SkyBoxUI/Classes/**/**/*.{storyboard,xib}"
    ]
  },
  "frameworks": [
    "UIKit",
    "Foundation",
    "AVFoundation"
  ],
  "libraries": [
    "sqlite3.0",
    "xml2"
  ],
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "/usr/include/libxml2"
  },
  "dependencies": {
    "AFNetworking": [
      "~> 3.1.0"
    ],
    "MJExtension": [
      "~> 3.0.13"
    ],
    "SDWebImage": [
      "~> 4.2.2"
    ],
    "Masonry": [
      "~> 1.1.0"
    ],
    "MBProgressHUD": [
      "~> 1.1.0"
    ],
    "FMDB": [
      "~> 2.7.2"
    ],
    "MMDrawerController": [
      "~> 0.6.0"
    ],
    "YHPDFImageLoader": [
      "~> 1.0.0"
    ],
    "KissXML": [
      "~> 5.2.0"
    ]
  },
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "4.0"
  },
  "prefix_header_contents": "#ifdef __OBJC__\n    #import \"SPDLANManager.h\"\n    #import \"SPAirScreenManager.h\"\n    #import \"SPDataManager.h\"\n    #import \"UIColor+SPImage.h\"\n    #import \"SPNavigationBar.h\"\n    #import \"SPSwitchBar.h\"\n    #import \"SPDeviceUtil.h\"\n    #import \"UIResponder+SPCategory.h\"\n    #import \"SPSingle.h\"\n    #import \"Commons.h\"\n    #import \"JRTTFTool.h\"\n    #import \"SPColorUtil.h\"\n    #import <Masonry/Masonry.h>\n    #import \"MBProgressHUD.h\"\n    #import \"SPBaseViewController.h\"\n    #import \"ServiceCall.h\"\n    #import <MJExtension/MJExtension.h>\n    #import <AFNetworking/AFNetworking.h>\n#endif /* __OBJC__*/"
}
